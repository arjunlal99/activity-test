{
    "manifest_version":2,
    "name": "Activity Monitor",
    "version": "1.0",

    "description": "Extension Activity Monitor",

    "permissions": [
        "activityLog","management","*://*/page/page.html"
    ],

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": "icons/icon.png"
    }

}